{
  "manifest_version": 3,
  "name": "Tab Tracker",
  "version": "1.0",
  "action": {
    "default_popup": "index.html"
  },
  "background": {
    "service_worker": "background.js"
  },
  "permissions": ["tabs",
    "activeTab",
    "storage",
    "idle",
    "unlimitedStorage",
    "alarms",
    "notifications",
    "offscreen"
  ],
  "icons": {
    "16": "16x16.png",
    "32": "32x32.png",
    "48": "48x48.png",
    "128": "128x128.png"
  }
}